     xmlns:aws="http://purl.oclc.org/NET/ssnx/meteo/aws#"
     xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://purl.org/iot/vocab/m3-lite#">
        <owl:versionInfo rdf:datatype="&xsd;decimal">0.3</owl:versionInfo>
        <dcterms:issued rdf:datatype="&xsd;date">2015-07-02</dcterms:issued>
        <dcterms:modified>2016-02-24</dcterms:modified>
        <dc:title xml:lang="en">The Machine-to-Machine Measurement (M3) Lite Ontology</dc:title>
        <dc:description xml:lang="en">The Machine-to-Machine Measurement (M3) Ontology is an unified language, nomenclature, dictionary which enables semantically annotating heterogeneous IoT data produced by heterogeneous devices measurements (sensors, RFID tags, etc.). We classify all devices, their measurements and the feature of interests (health, smart home, smart kitchen, environmental monitoring, etc.). By combining data from heterogeneous areas, we could propose new kinds of applications.</dc:description>
        <vann:preferredNamespacePrefix>m3-lite</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>http://purl.org/iot/vocab/m3-lite#</vann:preferredNamespaceUri>
        <vs:term_status>Work in progress</vs:term_status>
        <rdfs:comment xml:lang="en">M3 lite taxonomy is designed for the FIESTA-IOT H2020 EU project. We refactor, clean and simplify the M3 ontology designed by Eurecom (Amelie Gyrard). M3 ontology lite is aligned with the quantity taxonomy used by the SmartSantander testbed and the University of Surrey.</rdfs:comment>
        <dc:creator rdf:resource="http://purl.org/iot/vocab/amelie.gyrard@insight-centre.org"/>
        <dc:creator rdf:resource="http://purl.org/iot/vocab/m3-lite#tlmat.unican.es"/>
        <dc:creator rdf:resource="http://purl.org/iot/vocab/m3-lite#surrey.ac.uk"/>

    </owl:Ontology>
    
	<!-- 
		Last updated: February 24, 2016
		- ssn:Sensor replaced by ssn:SensingDevice
	    - I added the tagging device hierarchy (RFID, QRCode, NFC, Barcode) as a subclass of ssn:Device
		- update all FIESTA-IoT tools using the M3-lite taxonomy (resource registry)
		
		- TO DO: 	
		 ssn:FeatureOfInterest -> iot-lite:DomainOfInterest (should be updated in iot-lite)
	
	
	October 20, 2015
	M3-lite published online following the PURL namespace
	
	October 27, 2015
	New class for citypulse testbed VehicleCountSensor
	Add rdfs:comments for all subclasses of ssn:featureOfInterest
	Clean labels
	
	TO DO: Add comments for each concept/property
	
	-->

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://lod.taxonconcept.org/ontology/txn.owl#hasImage -->

    <owl:AnnotationProperty rdf:about="&txn;hasImage"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    


    <!-- http://xmlns.com/foaf/0.1/name -->

    <owl:AnnotationProperty rdf:about="&foaf;name"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasQuantityKind -->

    <owl:ObjectProperty rdf:about="&iot-lite;hasQuantityKind"/>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasUnit -->

    <owl:ObjectProperty rdf:about="&iot-lite;hasUnit"/>
    


    <!-- http://purl.org/iot/vocab/m3-lite#hasContext -->

    <owl:ObjectProperty rdf:about="&m3-lite;hasContext">
        <rdfs:label xml:lang="en">hasContext</rdfs:label>
        <rdfs:comment xml:lang="en">This property is used to classify devices by FeatureOfInterest (e.g., blood pressure sensor is used in healthcare)</rdfs:comment>
        <rdfs:domain rdf:resource="&ssn;Device"/>
        <rdfs:range rdf:resource="&ssn;FeatureOfInterest"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iot/vocab/m3-lite#produces -->

    <owl:ObjectProperty rdf:about="&m3-lite;produces">
        <rdfs:label xml:lang="en">produces</rdfs:label>
        <rdfs:comment xml:lang="en">This property is used to link the device to the QuantityKind (e.g., Thermometer sensor produces temperature) </rdfs:comment>
        <rdfs:domain rdf:resource="&ssn;Device"/>
        <rdfs:range rdf:resource="&qu;QuantityKind"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.qudt.org/qudt/owl/1.0.0/unit.owl#Bar -->

    <owl:Class rdf:about="&qudt_unit;Bar">
        <rdfs:label xml:lang="en">Bar</rdfs:label>
        <rdfs:subClassOf rdf:resource="&qu;Unit"/>
        <rdfs:seeAlso>http://qudt.org/vocab/unit#Bar</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Metric unit of atmospheric pressure equal to 14.50 pounds per square inch (lb/in2), 1.02 kilograms per square centimeter (kg/cm2), 29.53 inches of mercury (in Hg), or 0.9869 atmosphere.</rdfs:comment>
    </owl:Class>
    


    <!-- http://data.qudt.org/qudt/owl/1.0.0/unit.owl#DegreeAngle -->

    <owl:Class rdf:about="&qudt_unit;DegreeAngle">
        <rdfs:label xml:lang="en">Degree angle</rdfs:label>
        <rdfs:subClassOf rdf:resource="&qu;Unit"/>
        <rdfs:comment xml:lang="en">A plane angle unit which is equal to 1/360 of a full rotation or pi/180 rad.</rdfs:comment>
        <rdfs:seeAlso>http://qudt.org/vocab/unit#DegreeAngle</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://data.qudt.org/qudt/owl/1.0.0/unit.owl#ElectricCurrent -->

    <owl:Class rdf:about="&qudt_unit;ElectricCurrent">
        <rdfs:label xml:lang="en">Electric Current</rdfs:label>
        <rdfs:subClassOf rdf:resource="&qu;QuantityKind"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&iot-lite;hasUnit"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;Ampere"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:seeAlso>http://qudt.org/vocab/quantity#ElectricCurrent</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Electric current is the flow of electric charge. It is a base quantity in the International System of Units. Electric current is electric charge divided by time. Electric Current is the flow (movement) of electric charge. The amount of electric current through some surface, e.g., a section through a copper conductor, is defined as the amount of electric charge flowing through that surface over time.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/UNIS/fiware/iot-lite#ActuatingDevice -->

    <owl:Class rdf:about="&iot-lite;ActuatingDevice">
        <rdfs:label xml:lang="en">Actuating Device</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;Device"/>
        <rdfs:comment xml:lang="en">Actuators (e.g., switch on/off the light)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Device -->

    <owl:Class rdf:about="&ssn;Device"/>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest -->

    <owl:Class rdf:about="&ssn;FeatureOfInterest"/>
    



    


    <!-- http://purl.org/NET/ssnx/qu/qu#QuantityKind -->

    <owl:Class rdf:about="&qu;QuantityKind"/>
    


    <!-- http://purl.org/NET/ssnx/qu/qu#Unit -->

    <owl:Class rdf:about="&qu;Unit"/>
    


    <!-- http://purl.org/iot/vocab/m3-lite#ABS -->

    <owl:Class rdf:about="&m3-lite;ABS">
        <rdfs:label xml:lang="en">ABS (Anti-lock braking system)</rdfs:label>
        <rdfs:subClassOf rdf:resource="&iot-lite;ActuatingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;hasContext"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;Transportation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The actuator ABS (on/off)</rdfs:comment>
        <txn:hasImage rdf:resource="http://sensormeasurement.appspot.com/images/sensor/abs.png"/>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#Acceleration -->

    <owl:Class rdf:about="&m3-lite;Acceleration">
        <rdfs:subClassOf rdf:resource="&qu;QuantityKind"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&iot-lite;hasUnit"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;MeterPerSecondSquare"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The rate of change with time of the velocity vector of a particle.</rdfs:comment>
        <rdfs:seeAlso>http://purl.oclc.org/NET/ssnx/qu/dim#Acceleration</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#AccelerationInstantaneous -->

    <owl:Class rdf:about="&m3-lite;AccelerationInstantaneous">
        <rdfs:subClassOf rdf:resource="&m3-lite;Acceleration"/>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#Accelerometer -->

    <owl:Class rdf:about="&m3-lite;Accelerometer">
        <rdfs:label xml:lang="en">Accelerometer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;SensingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;hasContext"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;produces"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;Motion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;hasContext"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;Transportation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Accelerometer, Motion detector/Sensor. In iPhone 4, accelerometers are used to automatically determine the orientation in which the user is holding the phone (portrait or landscape).</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&cose;Accelerometer"/>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#ActivePower -->

    <owl:Class rdf:about="&m3-lite;ActivePower">
        <rdfs:subClassOf rdf:resource="&m3-lite;Power"/>
        <rdfs:seeAlso>http://purl.oclc.org/NET/ssnx/qu/quantity#activePower</rdfs:seeAlso>
        <rdfs:comment>The product of the voltage across a branch of an alternating-current circuit and the component of the electric current that is in phase with the voltage.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#Activity -->

    <owl:Class rdf:about="&m3-lite;Activity">
        <rdfs:label xml:lang="en">Activity recognition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&m3-lite;BuildingAutomation"/>
        <rdfs:comment xml:lang="en">Activity recognition as an IoT applicative domain</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#Agriculture -->

    <owl:Class rdf:about="&m3-lite;Agriculture">
        <rdfs:label xml:lang="en">Agriculture</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;FeatureOfInterest"/>
        <rdfs:comment xml:lang="en">Agriculture, Smart farm as an Internet of Things (IoT) applicative domain.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#Air -->

    <owl:Class rdf:about="&m3-lite;Air">
        <rdfs:label xml:lang="en">Air</rdfs:label>
        <rdfs:subClassOf rdf:resource="&m3-lite;Environment"/>
        <rdfs:comment xml:lang="en">Air as an IoT applicative domain</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#AirConditioner -->

    <owl:Class rdf:about="&m3-lite;AirConditioner">
        <rdfs:label xml:lang="en">Air conditioner</rdfs:label>
        <rdfs:subClassOf rdf:resource="&iot-lite;ActuatingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;hasContext"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;BuildingAutomation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;hasContext"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;Transportation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An actuator to automatically switch on/off the air conditioner.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#AirPollutantSensor -->

    <owl:Class rdf:about="&m3-lite;AirPollutantSensor">
        <rdfs:label xml:lang="en">Air Pollutant Sensor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;SensingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;hasContext"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;Environment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;produces"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;AirPollution"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Air Pollutant Sensor</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#AirPollution -->

    <owl:Class rdf:about="&m3-lite;AirPollution">
        <rdfs:label xml:lang="en">Air Pollution</rdfs:label>
        <rdfs:subClassOf rdf:resource="&qu;QuantityKind"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&iot-lite;hasUnit"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;EAQI"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Air Pollution, Air Quality</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#AirTemperature -->

    <owl:Class rdf:about="&m3-lite;AirTemperature">
        <rdfs:label xml:lang="en">Air Temperature, Weather Temperature, Ambient Temperature</rdfs:label>
        <rdfs:subClassOf rdf:resource="&m3-lite;Temperature"/>
        <rdfs:comment xml:lang="en">The temperature of the air that would be indicated by a thermometer exposed to the air at a location sheltered from direct solar radiation.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&openIoT;AirTemperature"/>
        <rdfs:seeAlso rdf:resource="&shw;Temperature"/>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#AirThermometer -->

    <owl:Class rdf:about="&m3-lite;AirThermometer">
        <rdfs:label xml:lang="en">Air Thermometer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;SensingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;hasContext"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;Agriculture"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;produces"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;AirTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Air Thermometer</rdfs:comment>
        <txn:hasImage rdf:resource="http://sensormeasurement.appspot.com/images/sensor/thermometer.png"/>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#AlarmSystem -->

    <owl:Class rdf:about="&m3-lite;AlarmSystem">
        <rdfs:label xml:lang="en">Alarm Systemr</rdfs:label>
        <rdfs:subClassOf rdf:resource="&iot-lite;ActuatingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;hasContext"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;Transportation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&m3-lite;hasContext"/>
                <owl:someValuesFrom rdf:resource="&m3-lite;BuildingAutomation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An actuator to automatically switch on/off the alarm system.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#AlcoholLevel -->

    <owl:Class rdf:about="&m3-lite;AlcoholLevel">
        <rdfs:label xml:lang="en">Alcohol Level</rdfs:label>
        <rdfs:subClassOf rdf:resource="&qu;QuantityKind"/>
        <rdfs:comment xml:lang="en">Alcohol Level</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iot/vocab/m3-lite#AlcoholLevelSensor -->

