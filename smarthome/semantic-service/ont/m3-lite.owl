     xmlns:iot-lite="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#"
     xmlns:movie="http://www.movieontology.org/2010/01/movieontology.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:muo="http://purl.oclc.org/NET/muo/muo#"
     xmlns:dogont="http://elite.polito.it/ontologies/dogont.owl#"
