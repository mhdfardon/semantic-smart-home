    <!-- http://umbel.org/umbel/sc/Fruit -->

    <owl:Class rdf:about="&sc;Fruit">
        <owl:equivalentClass rdf:resource="#Fruit"/>
        <rdfs:comment xml:lang="en"
            >Entity that makes the relationship with UMBEL adds meaning to the concept of fruit</rdfs:comment>
        <rdfs:comment xml:lang="es"
            >Entidad que hace la relacion con UMBEL adiciona significado al concepto de Fruta.</rdfs:comment>
        <rdfs:comment xml:lang="fr"
