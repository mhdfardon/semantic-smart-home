	<owl:Class rdf:ID="Person">		
		<rdfs:label xml:lang="en">Person</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>	
		<rdfs:seeAlso rdf:resource="&foaf;Person"/>
		<rdfs:seeAlso rdf:resource="&registry;Person"/>
		<rdfs:seeAlso rdf:resource="&Person_SH_Ontology;Person"/>
	</owl:Class>
	
	<owl:Class rdf:ID="Patient">		
		<rdfs:label xml:lang="en">Patient</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>	
		<rdfs:subClassOf rdf:resource="#Person"/>	
		<rdfs:seeAlso rdf:resource="&registry;Patient"/>
		<rdfs:seeAlso rdf:resource="&Person_SH_Ontology;Patient"/>
	</owl:Class>	
	
	<owl:ObjectProperty rdf:ID="likes">
  		<rdfs:domain rdf:resource="#Person"/>
 		<rdfs:range  rdf:resource="#Food"/>
		<rdfs:label xml:lang="en">likes</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:ID="hates">
  		<rdfs:domain rdf:resource="#Person"/>
 		<rdfs:range  rdf:resource="#Food"/>
		<rdfs:label xml:lang="en">hates</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:ID="hasDisease">
 		<rdfs:range  rdf:resource="#Disease"/>
