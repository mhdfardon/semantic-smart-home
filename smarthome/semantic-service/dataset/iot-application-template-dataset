   		<m3:hasContext rdf:resource="&m3;Weather"/>
   		<m3:hasContext rdf:resource="&m3;Transportation"/>
		<rdfs:label xml:lang="en">Temperature, Transportation and Safety Device</rdfs:label>
		<rdfs:comment xml:lang="en">IoT application to suggest safety devices according to the outisde temperature (e.g., rainy -> low beam)</rdfs:comment>    
	    <m3:hasM2MDevice rdf:resource="&m3;Thermometer"/>	
