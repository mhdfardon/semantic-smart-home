       	<!-- ################################################ AGRICULTURE RULES ############################################## -->
       	
    <m3:Rule rdf:about="AirStressLevelLow">
		<rdfs:label xml:lang="en">AirStress level low</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:AirTemperature lessThan 12 degC AND m3:AirHumidity lessThan 25% then AirStress level = LOW</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;AirThermometer"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;HumiditySensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;CA4IoTPerera2013"/>	  
    </m3:Rule>
    
    <m3:Rule rdf:about="AirStressLevelHigh">
		<rdfs:label xml:lang="en">AirStress level low</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:AirTemperature greaterThan 12 degC AND m3:AirHumidity greaterThan 25% then AirStress level = HIGH</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;AirThermometer"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;HumiditySensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;CA4IoTPerera2013"/>	  
    </m3:Rule>
    
     <m3:Rule rdf:about="PlantInfected">
		<rdfs:label xml:lang="en">Plant infected or not (Phytophtora disease)</rdfs:label>
		<rdfs:comment xml:lang="en">IF AirStress=high and leafWetness greaterThan 50 then phytophtora disease = infected else not infected</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;AirThermometer"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;HumiditySensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;CA4IoTPerera2013"/>	  
    </m3:Rule>
       	
       	<!-- ################################################ RULE ACTIVITY LIMING CHEN ############################################## -->
    <m3:Rule rdf:about="MakeCoffee">
		<rdfs:label xml:lang="en">MakeCoffee</rdfs:label>
		<rdfs:comment xml:lang="en">if hasUse = m3:Cup and hasUse = m3:CoffeeMachine and location=Kitchen then home:activity = home-dataset:MakeCoffee</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Cup"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;CoffeeMachine"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;LimingChen"/>	
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;GyroscopeSensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	   
    </m3:Rule>
    
      <m3:Rule rdf:about="BrushTeeth">
		<rdfs:label xml:lang="en">BrushTeeth</rdfs:label>
		<rdfs:comment xml:lang="en">if hasUse = m3:BrushTeeth then activity = home-dataset:BrushTeeth</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_BrushTeeth"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;LimingChen"/>	  
    </m3:Rule>
    
    <m3:Rule rdf:about="MakeChocolate">
		<rdfs:label xml:lang="en">MakeChocolate</rdfs:label>
		<rdfs:comment xml:lang="en">if hasUse = m3:RFID_Cup then activity = home-dataset:MakeChocolate</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Cup"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;LimingChen"/>	  
    </m3:Rule>
    
    <m3:Rule rdf:about="MakeTea">
		<rdfs:label xml:lang="en">MakeTea</rdfs:label>
		<rdfs:comment xml:lang="en">if hasUse = m3:RFID_Cup and m3:RFID_TeaBag m3:Boiler then activity = home-dataset:MakeChocolate</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Cup"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_TeaBag"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Boiler"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;LimingChen"/>	  
    </m3:Rule>
    
     <m3:Rule rdf:about="MakePasta">
		<rdfs:label xml:lang="en">MakePasta</rdfs:label>
		<rdfs:comment xml:lang="en">if hasUse = m3:RFID_PASTA and m3:RFID_Pan then activity = home-dataset:MakeChocolate</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Paste"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Pan"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;LimingChen"/>	  
    </m3:Rule>
       	       
	 <!-- ################################################ RULE ACTIVITY KONLAKORN ############################################## -->
	 
	
     <m3:Rule rdf:about="SweepingFloor">
		<rdfs:label xml:lang="en">Sweeping_the_floor</rdfs:label>
		<rdfs:comment xml:lang="en">if hasUse = m3:RFID_Broom then home:activity = sweeping the floor</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Broom"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;GyroscopeSensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	  
    </m3:Rule>
    
    <m3:Rule rdf:about="WorkingComputer">
		<rdfs:label xml:lang="en">Working_on_a_computer</rdfs:label>
		<rdfs:comment xml:lang="en">if hasUse = M3:Computer then home:activity = working on a computer</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Computer"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	    
    </m3:Rule>
    
     <m3:Rule rdf:about="SittingToilet">
		<rdfs:label xml:lang="en">Sitting_on_the_toilet</rdfs:label>
		<rdfs:comment xml:lang="en">if location = bathroom and has_posture= sit and hasUse = M3:Lavatory then home:activity = sitting on the toilet</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Lavatory"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;UltrasonicSensor"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;WaterFlow"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	  
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>  
    </m3:Rule>
    
      <m3:Rule rdf:about="WashDishes">
		<rdfs:label xml:lang="en">Wash_dishes</rdfs:label>
		<rdfs:comment xml:lang="en">if location = kitchen and hasUse = M3:Sink then home:activity = Wash_dishes</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Sink"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;WaterFlow"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	 
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>   
    </m3:Rule>
    
    <m3:Rule rdf:about="ScrubbingFloor">
		<rdfs:label xml:lang="en">Scrubbing_the_floor</rdfs:label>
		<rdfs:comment xml:lang="en">if hasUse = M3:RFID_Mop then home:activity = scrubbing the floor</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Mop"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;GyroscopeSensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	    
    </m3:Rule>
    
   <m3:Rule rdf:about="ReadingBook">
		<rdfs:label xml:lang="en">Reading_a_book</rdfs:label>
		<rdfs:comment xml:lang="en">if hasPosture = Sit and hasUse = M3:RFID_Book then home:activity = home-dataset:ReadingBook</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Book"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;UltrasonicSensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	    
    </m3:Rule>
    
    <m3:Rule rdf:about="WatchingTV">
		<rdfs:label xml:lang="en">WathchingTV</rdfs:label>
		<rdfs:comment xml:lang="en">if hasUse = M3:TV then home:activity = home-dataset:WatchTV</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;TV"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	
	    <m3:fromM2MApplication rdf:resource="&lov4iot;LimingChen"/>	    
    </m3:Rule>
    
    <m3:Rule rdf:about="LyingDownRelaxing">
		<rdfs:label xml:lang="en">Lying_down_relaxing</rdfs:label>
		<rdfs:comment xml:lang="en">if location = living room and hasUse = m3:RFID_Sofa and hasPosture=Lie-down then home:activity = home-dataset:LyingDownRelaxing</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Sofa"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	 
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;PressureSensor"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;UltrasonicSensor"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>   
    </m3:Rule>
    
     <m3:Rule rdf:about="HaveBath">
		<rdfs:label xml:lang="en">Have a bath, taking a bath</rdfs:label>
		<rdfs:comment xml:lang="en">if location = bathroom and hasUse= m3:Shower then home:activity = home-dataset:HaveBath</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Shower"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;WaterFlow"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	  
	    <m3:fromM2MApplication rdf:resource="&lov4iot;LimingChen"/>	
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>  
    </m3:Rule>

    
     <m3:Rule rdf:about="Cooking">
		<rdfs:label xml:lang="en">Cooking</rdfs:label>
		<rdfs:comment xml:lang="en">if location = kitchen and hasUse = m3:Fridge then home:activity = home-dataset:Cooking</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Fridge"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Magnetometer"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	    
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>
    </m3:Rule>
    
      <m3:Rule rdf:about="EatingDrinking">
		<rdfs:label xml:lang="en">Eating_or_drinking</rdfs:label>
		<rdfs:comment xml:lang="en">if location = kitchen and has_food= m3:RFID_Food and hasPosture=Sit then home:activity = home-dataset:Eating or home-dataset:Drinking</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Food"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Magnetometer"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;UltrasonicSensor"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	    
    </m3:Rule>
    
    <m3:Rule rdf:about="SleepingKonlakorn">
		<rdfs:label xml:lang="en">Sleeping Konlakorn</rdfs:label>
		<rdfs:comment xml:lang="en">if location = bedroom hasUse= m3:RFID_Bed and hasPosture= lieDown then home:activity = home-dataset:Sleeping</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;RFID_Bed"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;PressureSensor"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;UltrasonicSensor"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Konlakorn"/>	    
    </m3:Rule>
    
      <m3:Rule rdf:about="SleepingRiboni">
		<rdfs:label xml:lang="en">Sleeping Riboni</rdfs:label>
		<rdfs:comment xml:lang="en">if location = bedroom AND m3:Luminosity lessThan 40 lux AND sound lessThan 30 db then home:activity = home-dataset:Sleeping</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;LightSensor"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;SoundSensor"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Riboni"/>	    
    </m3:Rule>
    
    <m3:Rule rdf:about="LightPresence">
		<rdfs:label xml:lang="en">LightPresence</rdfs:label>
		<rdfs:comment xml:lang="en">if presence=yes THEN turn light actuator on</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;LightSensor"/>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;OccupancyDetector"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Jacquet"/>	 
	    <m3:hasUrlRule rdf:resource="&lorHome;"/> 	   
    </m3:Rule>
    
   <!-- ################################################ RULE HEALTH ############################################## -->
    
    <m3:Rule rdf:about="HighFever">
		<rdfs:label xml:lang="en">HighFever</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:BodyTemperature greaterThan 38 m3:DegreeCelsius AND lessThan 39 m3:DegreeCelsius THEN HighFever</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Obaid"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;BodyThermometer"/> 
	   	<m3:hasUrlRule rdf:resource="&lorHealth;"/>     
    </m3:Rule>
    
     <m3:Rule rdf:about="CriticallyHighFever">
		<rdfs:label xml:lang="en">CriticallyHighFever</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:BodyTemperature greaterThan 39 m3:DegreeCelsius THEN CriticallyHighFever</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Obaid"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;BodyThermometer"/>    
	   	<m3:hasUrlRule rdf:resource="&lorHealth;"/> 
    </m3:Rule>
    
    <m3:Rule rdf:about="NormalFever">
		<rdfs:label xml:lang="en">NormalFever</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:BodyTemperature greaterThan 36 m3:DegreeCelsius AND lessThan 38 m3:DegreeCelsius THEN NormalFever</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Obaid"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;BodyThermometer"/> 
	   	<m3:hasUrlRule rdf:resource="&lorHealth;"/>    
    </m3:Rule>
    
    <m3:Rule rdf:about="MildHypothermia">
		<rdfs:label xml:lang="en">MildHypothermia</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:BodyTemperature greaterThan 33 m3:DegreeCelsius AND lessThan 35 m3:DegreeCelsius THEN MildHypothermia</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Jara"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;BodyThermometer"/>  
		<m3:hasUrlRule rdf:resource="&lorHealth;"/>  
    </m3:Rule>
    
    <m3:Rule rdf:about="ModerateHypothermia">
		<rdfs:label xml:lang="en">ModerateHypothermia</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:BodyTemperature greaterThan 28 m3:DegreeCelsius AND lessThan 32 m3:DegreeCelsius THEN ModerateHypothermia</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Jara"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;BodyThermometer"/>    
	   	<m3:hasUrlRule rdf:resource="&lorHealth;"/> 
    </m3:Rule>
    
     <m3:Rule rdf:about="SevereHypothermia">
		<rdfs:label xml:lang="en">SevereHypothermia</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:BodyTemperature lessThan 28 m3:DegreeCelsius THEN SevereHypothermia</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Jara"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;BodyThermometer"/>  
	   	<m3:hasUrlRule rdf:resource="&lorHealth;"/>   
    </m3:Rule>
    
    <m3:Rule rdf:about="Hypertension">
		<rdfs:label xml:lang="en">Hypertension</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:SystolicBloodPressure greaterThan 140 m3:mmHg AND m3:DiastolicBloodPressure greaterThan 90 m3:mmHg THEN Hypertension</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Hassanpour"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;BloodPressureSensor"/>  
	   	<m3:hasUrlRule rdf:resource="&lorHealth;"/>   
    </m3:Rule>
    
    <m3:Rule rdf:about="NormalSystolicBloodPressureBravo">
		<rdfs:label xml:lang="en">NormalSystolicBloodPressure Bravo</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:SystolicBloodPressure greaterThan 120 m3:mmHg AND m3:SystolicBloodPressure lessThan 160 m3:mmHg THEN NormalSystolicBloodPressure</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Bravo"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;BloodPressureSensor"/>   
	   	<m3:hasUrlRule rdf:resource="&lorHealth;"/>  
    </m3:Rule>
    
    <m3:Rule rdf:about="NormalCholesterol">
		<rdfs:label xml:lang="en">NormalCholesterol</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Cholesterol greaterThan 4 m3:MmolPerLiter AND lessThan 6 m3:MmolPerLiter THEN NormalCholesterol</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Bravo"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;CholesterolSensor"/>  
	   	<m3:hasUrlRule rdf:resource="&lorHealth;"/>   
    </m3:Rule>
    
    <m3:Rule rdf:about="Diabete">
		<rdfs:label xml:lang="en">Diabete</rdfs:label>
		<rdfs:comment xml:lang="en">IF glucose greaterThan 126 GramPerLiter AND beforeEating THEN Diabete</rdfs:comment>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Hassanpour"/>	   
	   	<m3:ruleUsingM2MDevice rdf:resource="&m3;Glucometer"/>   
	   	<m3:hasUrlRule rdf:resource="&lorHealth;"/>  
    </m3:Rule>
    
