    <!-- ################################################ RODRIGUEZ RULES : HUMISITY, LIGHTING, PRESSURE, TEMPERATURE, NOISE LEVEL ############################################## -->
    
    <!--  Humidity -->
    
     <m3:Rule rdf:about="HighHumidityRodriguez">
