     <m3:Rule rdf:about="LowTemperatureRodriguez">
		<rdfs:label xml:lang="en">LowTemperature</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Temperature greaterThan -10 and LowerThan 0 m3:DegreeCelsius THEN LowTemperature </rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Thermometer"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Rodriguez"/>  	    
    </m3:Rule>
    
       <!--  Noise level -->
    
      <m3:Rule rdf:about="HighSoundRodriguez">
		<rdfs:label xml:lang="en">HighSound</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Sound greaterThan 90 and LowerThan 110 m3:Decibel THEN HighSound</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;SoundSensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Rodriguez"/>
	    <m3:hasUrlRule rdf:resource="&lorHome;"/>   	    
    </m3:Rule>
    
     <m3:Rule rdf:about="MediumSoundRodriguez">
		<rdfs:label xml:lang="en">MediumSound</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Sound greaterThan 40 and LowerThan 90 m3:Decibel THEN MediumSound</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;SoundSensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Rodriguez"/>  
	    <m3:hasUrlRule rdf:resource="&lorHome;"/> 	    
    </m3:Rule>
    
     <m3:Rule rdf:about="LowSoundRodriguez">
		<rdfs:label xml:lang="en">LowSound</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Sound greaterThan 30 and LowerThan 40 m3:Decibel THEN LowSound</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;SoundSensor"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;Rodriguez"/>  
	    <m3:hasUrlRule rdf:resource="&lorHome;"/>  		    
    </m3:Rule>
    
       <!-- ################################################ FISHERY TEMPERATURE AND SALINITY ############################################## -->
    
    
    <m3:Rule rdf:about="TemperatureSuFishery">
		<rdfs:label xml:lang="en">Water Temperature for fish</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:WaterTemperature greaterThan 27 OR LowerThan 18 m3:DegreeCelsius THEN m3:Alert</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;Thermometer"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;SuFishery"/>   		    
    </m3:Rule>
    
      <m3:Rule rdf:about="SalinitySuFishery">
		<rdfs:label xml:lang="en">Salinity for fish</rdfs:label>
		<rdfs:comment xml:lang="en">IF m3:Salinity greaterThan 0.033 OR LowerThan 0.008 THEN m3:Alert</rdfs:comment>
	    <m3:ruleUsingM2MDevice rdf:resource="&m3;SaltMeter"/>
	    <m3:fromM2MApplication rdf:resource="&lov4iot;SuFishery"/>   		    
    </m3:Rule>
    
</rdf:RDF>