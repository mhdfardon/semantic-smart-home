    	<rdfs:label xml:lang="en">Peach</rdfs:label>
        <rdf:type rdf:resource="&food;Peach"/>
        <rdf:type rdf:resource="&naturopathy;Food"/>
        <m3:hasRecommendation rdf:resource="Summer"/>
        <naturopathy:hasNutrient rdf:resource="Protein"/>
    </naturopathy:Fruit>
            
