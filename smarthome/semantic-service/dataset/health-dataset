		<rdfs:comment xml:lang="en"></rdfs:comment>
		<dc:description xml:lang="en">LowSPO2, IF m3:SPO2 lessThan 190 m3:Percent THEN LowSPO2 [Hristoskova 2014]</dc:description>				
	</health:Symptom>
	
	<!-- ################################################ CHOLESTEROL ############################################################################ -->
	
